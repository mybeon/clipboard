{
    "compilerOptions": {
        "target": "es2016",
        "module": "commonjs",
        "outDir": "./app/electron/build",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true
    },
    "include": ["app/electron/src/**/*.ts"],
    "exclude": ["node_modules"]
}
